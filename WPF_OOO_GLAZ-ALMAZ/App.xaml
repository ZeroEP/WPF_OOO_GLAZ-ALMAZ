<Application x:Class="WPF_OOO_GLAZ_ALMAZ.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPF_OOO_GLAZ_ALMAZ"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="Picture" UriSource="/Static/Logo.png" />
        <SolidColorBrush x:Key="BaseBackgroud" Color="#FF9BD1E5" />
        <SolidColorBrush x:Key="AdditionalBackgroud" Color="#3E6680" />
        <SolidColorBrush x:Key="AccentBackgroud" Color="#1C0221" />
    </Application.Resources>
</Application>
